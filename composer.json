{
  "name": "tourze/redis-dedicated-connection-bundle",
  "description": "为服务自动创建和注入专用的 Redis 连接，支持属性、标签、环境变量等多种配置方式",
  "type": "symfony-bundle",
  "keywords": [
    "symfony",
    "dedicated-connection"
  ],
  "license": "MIT",
  "require": {
    "ext-filter": "*",
    "ext-redis": "*",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/symfony-runtime-context-bundle": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\RedisDedicatedConnectionBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\RedisDedicatedConnectionBundle\\Tests\\": "tests"
    }
  }
}